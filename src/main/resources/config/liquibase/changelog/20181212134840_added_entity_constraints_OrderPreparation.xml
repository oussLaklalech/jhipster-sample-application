<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity OrderPreparation.
    -->
    <changeSet id="20181212134840-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="consumer_id"
                                 baseTableName="order_preparation"
                                 constraintName="fk_order_preparation_consumer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="consumer"/>

        <addForeignKeyConstraint baseColumnNames="dish_id"
                                 baseTableName="order_preparation"
                                 constraintName="fk_order_preparation_dish_id"
                                 referencedColumnNames="id"
                                 referencedTableName="dish"/>

    </changeSet>
</databaseChangeLog>
